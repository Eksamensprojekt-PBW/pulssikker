<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>erhverv</title>
</head>
<body>

    <nav>
        <div>
            <a href="/"><h1>PulsSikker</h1></a>
            <p>Førstehjælps kurser</p>
        </div>
        <ul>
            <li><a href="/">PulsSikker</a></li>
            <li><a href="/erhverv">erhverv</a></li>
            <li><a href="/privat">privat</a></li>
            <li><a href="/om">om os</a></li>
            <li><a href="/kontakt">kontakt</a></li>
        </ul>
    </nav>
    
    <h1>Business Courses</h1>
    <div class="course-list">
        <% courses.forEach(function(course) { %>
            <div class="course">
                <h2><%= course.title %></h2>
                <p><strong>Duration:</strong> <%= course.duration %> hours</p>
                <p><strong>Description:</strong> <%= course.description %></p>
                <p><strong>Price:</strong> <%= course.price %> <%= course.currency %> (excl. VAT)</p>
            </div>
        <% }); %>
    </div>

</body>
</html>